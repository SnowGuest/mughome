{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "types/env.d.ts",
    "src/**/*.vue",
    "src/**/*.json",
    "types/*.d.ts",
  ],
  "compilerOptions": {
    "jsxImportSource": "vue",
    "moduleResolution": "Node",
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  }
}